<Window x:Class="DNF.Panele.Logowanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Logowanie" Height="600" Width="1000">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/DNF;component/Images/bg.jpg"/>
        </Grid.Background>
        <TextBlock HorizontalAlignment="Left" Margin="272,227,0,0" TextWrapping="Wrap" Text="Login" VerticalAlignment="Top" FontSize="23" Foreground="#FFC9C2C2"/>
        <TextBlock HorizontalAlignment="Left" Margin="272,282,0,0" TextWrapping="Wrap" Text="Hasło" VerticalAlignment="Top" FontSize="23" Foreground="#FFC9C2C2"/>
        <TextBox Name="loginTb" HorizontalAlignment="Left" Height="41" Margin="382,226,0,0" TextWrapping="Wrap" FontSize="23"  VerticalAlignment="Top" Width="258" Background="#FF9B9B9B"/>
        <PasswordBox Name="hasloTb" HorizontalAlignment="Left" Height="41" Margin="382,281,0,0" FontSize="23"  VerticalAlignment="Top" Width="258" Background="#FF9B9B9B"/>
        <TextBlock HorizontalAlignment="Left" Margin="214,80,0,0" TextWrapping="Wrap" Text="Dolnośląski festiwal nauki" VerticalAlignment="Top" FontSize="60" Foreground="#FFC9C2C2" FontFamily="Quirky Nots"/>
        <Button Content="Zaloguj" HorizontalAlignment="Left" Margin="490,346,0,0" VerticalAlignment="Top" Width="150" Background="{x:Null}" Height="28" Click="zaloguj" Foreground="#FFB9B9B9"/>

    </Grid>
</Window>
